   "source": [
    "import datetime\n",
    "from datetime import timedelta\n",
    "\n",
    "def daysAlive(year,month,day):\n",
    "\n",
    "    dayBorn = datetime.date(year,month,day)\n",
    "    tday = datetime.date.today()\n",
    "    numDays = tday - dayBorn\n",
    "    \n",
    "    return(numDays)"
   ]
